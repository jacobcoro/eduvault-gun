{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": false,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2020",
			"DOM"
		],
		"preserveValueImports": false,
		"moduleResolution": "node",
		"module": "commonjs",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2020",
		"types": [
			"jest"
		]
	}
}